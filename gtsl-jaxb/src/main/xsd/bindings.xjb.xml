<jxb:bindings version="1.0" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

  <jxb:bindings schemaLocation="ts_119612v020101_sie_xsd.xsd" node="/xs:schema">
    <jxb:schemaBindings>
      <jxb:package name="eu.futuretrust.gtsl.jaxb.sie"/>
      <jxb:nameXmlTransform>
        <jxb:typeName suffix="JAXB"/>
      </jxb:nameXmlTransform>
    </jxb:schemaBindings>
  </jxb:bindings>

  <jxb:bindings schemaLocation="XAdES.xsd" node="/xs:schema">
        <jxb:schemaBindings>
            <jxb:package name="eu.futuretrust.gtsl.jaxb.xades"/>
            <jxb:nameXmlTransform>
                <jxb:elementName prefix="xades" />
                <jxb:typeName suffix="JAXB"/>
            </jxb:nameXmlTransform>
        </jxb:schemaBindings>
  </jxb:bindings>

  <jxb:bindings schemaLocation="http://www.w3.org/TR/2002/REC-xmldsig-core-20020212/xmldsig-core-schema.xsd"
                node="/xs:schema">
    <jxb:schemaBindings>
        <jxb:package name="eu.futuretrust.gtsl.jaxb.xmldsig"/>
        <jxb:nameXmlTransform>
            <jxb:elementName prefix="ds"/>
            <jxb:typeName suffix="JAXB"/>
        </jxb:nameXmlTransform>
    </jxb:schemaBindings>
    </jxb:bindings>

  <jxb:bindings schemaLocation="ts_119612v020101_additionaltypes_xsd.xsd" node="/xs:schema">
    <jxb:schemaBindings>
      <jxb:package name="eu.futuretrust.gtsl.jaxb.additionaltypes"/>
      <jxb:nameXmlTransform>
        <jxb:typeName suffix="JAXB"/>
      </jxb:nameXmlTransform>
    </jxb:schemaBindings>
    <jxb:bindings schemaLocation="https://uri.etsi.org/19612/v1.2.1/tsl.xsd" node="/xs:schema">
      <jxb:schemaBindings>
        <jxb:package name="eu.futuretrust.gtsl.jaxb.tsl"/>
        <jxb:nameXmlTransform>
          <jxb:typeName suffix="JAXB"/>
        </jxb:nameXmlTransform>
      </jxb:schemaBindings>
    </jxb:bindings>
  </jxb:bindings>

</jxb:bindings>
